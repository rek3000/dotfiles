# Increase volume
"pamixer -i 5"
   XF86AudioRaiseVolume

# Decrease volume
"pamixer -d 5"
   XF86AudioLowerVolume

# Mute volume
"pamixer -t"
   XF86AudioMute

"light -A 5"
   XF86MonBrightnessUp 

"light -A 5"
   MOD4 + F12

"light -U 5"
   MOD4 + F11

"light -U 5"
   XF86MonBrightnessDown

"scmenu"
   Print

"saudio"
   MOD4 + F9

"power"
   MOD4 + SHIFT + Home
