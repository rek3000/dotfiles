#FnIncBright
"xbacklight -inc 5"
    m:0x10 + c:233
    Mod2 + XF86MonBrightnessUp 

#FnDecBright
"xbacklight -dec 5"
    m:0x10 + c:232
    Mod2 + XF86MonBrightnessDown 

#NorIncBright
"xbacklight -inc 5"
    m:0x50 + c:96
    Mod2+Mod4 + F12 

#NorDecBright
"xbacklight -dec 5"
    m:0x50 + c:95
    Mod2+Mod4 + F11 

#FnIncVolume
"pamixer -i 2"
    m:0x0 + c:123
    XF86AudioRaiseVolume 

#NorIncVolume
"pamixer -i 2"
    m:0x40 + c:69
    Mod4 + F3 

#FnDecVolume
"pamixer -d 2"
    m:0x0 + c:122
    XF86AudioLowerVolume 

#NorDecVolume
"pamixer -d 2"
    m:0x40 + c:68
    Mod4 + F2 

#FnToggleVolume
"pamixer -t"
    m:0x0 + c:121
    XF86AudioMute 

#NorToggleVolume
"pamixer -t"
    m:0x40 + c:67
    Mod4 + F1 

#PrintScr
"scmenu"
    m:0x0 + c:107
    Print 

#switch_audio
"saudio"
    m:0x40 + c:75
    Mod4 + F9 

#rofi
"rofi -show run"
    m:0x40 + c:42
    Mod4 + g 

#Suspend
"prompt.sh Do\ you\ want\ to\ suspend? systemctl\ suspend"
    m:0x44 + c:119
    Control+Mod4 + Delete 

#Reboot
"prompt.sh Do\ you\ want\ to\ reboot? reboot"
    m:0x44 + c:110
    Control+Mod4 + Home 

#Shutdown
"prompt.sh Do\ you\ want\ to\ poweroff? poweroff"
    m:0x44 + c:115
    Control+Mod4 + End 

